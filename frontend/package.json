{
  "name": "pm-journey-frontend",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev:next": "pnpm --filter @pm-journey/next-legacy dev",
    "build:next": "pnpm --filter @pm-journey/next-legacy build",
    "start:next": "pnpm --filter @pm-journey/next-legacy start",
    "lint:next": "pnpm --filter @pm-journey/next-legacy lint",
    "test:next": "pnpm --filter @pm-journey/next-legacy test",
    "dev:app": "pnpm --filter @pm-journey/app dev",
    "build:app": "pnpm --filter @pm-journey/app build",
    "preview:app": "pnpm --filter @pm-journey/app preview",
    "lint": "pnpm --recursive lint",
    "test": "pnpm --recursive test",
    "e2e": "pnpm --filter @pm-journey/next-legacy e2e"
  }
}
